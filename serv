    public MerchantResponse<AlertManagementResponse> getUnreadAlertsByMId(String mId) {
        log.info("Fetching unread alert descriptions for mId: {}", mId);
        List<AlertManagementResponse> alertManagementResponses = alertDao.getLatestAlertDescription(mId, false, PageRequest.of(DEFAULT_PAGE_NUMBER, DEFAULT_ALERT_PAGE_SIZE, Sort.by(Sort.Direction.DESC, "createdAt")));
        return MerchantResponse.<AlertManagementResponse>builder().data(alertManagementResponses).status(RESPONSE_SUCCESS).total((long) alertManagementResponses.size()).build();

    }


