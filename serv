  public MerchantUserResponse updateUserStatusByUserName(String userName, UserStatus userStatus, MerchantUserUpdateRequest merchantUserUpdateRequest) {
        MerchantUser merchantUser = merchantUserRepository.findByUserNameOrEmailOrMobilePhone(userName, userName, userName).get();
        merchantUser.setStatus(userStatus);
        merchantUser.setRemark(merchantUserUpdateRequest.getRemark());
        return merchantMapper.mapMerchantUserEntityToResponse(merchantUserRepository.save(merchantUser));
    }
