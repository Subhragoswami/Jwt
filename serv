   @PostMapping
    @Operation(summary = "Pricing  Calculation", description = "")
    public EPayResponseEntity<EncryptedResponse> calculatePricing(@RequestBody EncryptedRequest encryptedRequest) {
        logger.info("Request received for calculate price : {}", encryptedRequest);
        return merchantPricingService.calculatePricingEncrypted(encryptedRequest);
    }
