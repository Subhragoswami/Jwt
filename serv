    @Transactional
    public void cancelScheduler(UUID id){
        if(reportScheduleManagementRepository.existsById(id)) {
            reportScheduleManagementRepository.updateCancelById(id);
        }
    }

@Modifying
    @Query("UPDATE ReportScheduleManagement r SET r.cancel = false WHERE r.id = id")
    void updateCancelById(@Param("id") UUID id);
