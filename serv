 @GetMapping("/{keyType}/{mId}")
    @Operation(summary = "Get list of keys for given MID", description = "Get list of keys for given MID")
    public KMSResponse<KeyManagementDto> getListOfKeys(@PathVariable KeyType keyType, @PathVariable String mId, @PageableDefault(size = 50, page = 0) Pageable pageable) {
        logger.info("Fetching list of keys for Merchant with ID: {} and key type: {}", mId, keyType);
        return keyManagementService.getKeysByMerchantId(keyType, mId, pageable);
    }

public enum KeyType {
    AEK, MEK, KEK, API
}

