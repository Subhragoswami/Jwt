  private static String generateCSV(List<String> headers, List<List<Object>> objects) {
        StringBuilder csvContent = new StringBuilder();

        csvContent.append(String.join(",", headers));
        csvContent.append("\n");
        for (List<Object> rowData : objects) {
            for (Object columnData : rowData) {
                csvContent.append(ObjectUtils.isNotEmpty(columnData) ? columnData : StringUtils.EMPTY);
                csvContent.append(",");
            }
            csvContent.append("\n");
        }
        return csvContent.toString();
    }
