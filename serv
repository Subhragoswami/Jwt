@Mapper(componentModel = "spring")
public interface CaptchaMapper {

    @Mapping(target = "captchaImage", source = "base64Image")
    @Mapping(target = "requestType", expression = "java(captchaDto.getRequestType().name())")
    CaptchaResponse mapCaptchaDtoToCaptchaResponse(CaptchaDto captchaDto, String base64Image);
}