   @GetMapping("/bank")
    public MerchantResponse<BankResponse> getAllBanks() {
        return adminService.getAllBanks();
    }

    @GetMapping("/bank/branch/{bankId}")
    public MerchantResponse<BranchResponse> getAllBranches(@PathVariable String bankId) {
        return adminService.getAllBranches(bankId);
    }

   public MerchantResponse<BankResponse> getAllBanks(){
        log.info("Fetching bank ");
        List<BankResponse> bankResponse = adminDao.getAllBanks();
        log.info("Fetched bank details: {}", bankResponse);
        return MerchantResponse.<BankResponse>builder().data(bankResponse).build();
    }

    public MerchantResponse<BranchResponse> getAllBranches(String bankId){
        log.info("Fetching bank ");
        List<BranchResponse> branchResponse = adminDao.getAllBranches(bankId);
        log.info("Fetched bank details: {}", branchResponse);
        return MerchantResponse.<BranchResponse>builder().data(branchResponse).build();
    }

  public List<BankResponse> getAllBanks() {
        return adminServicesClient.getAllBanks().getData();
    }

    public List<BranchResponse> getAllBranches(String bankId) {
        return adminServicesClient.getAllBranches(bankId).getData();
    }



