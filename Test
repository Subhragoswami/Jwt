    @Test
    void searchRefundBookingRequest() {
        List<RefundBooking> entityList = Collections.singletonList(new RefundBooking());
        Page<RefundBooking> entityPage = new PageImpl<>(entityList, pageable, 1L);

        List<RefundBookingDto> dtoList = Collections.singletonList(new RefundBookingDto());
        Page<RefundBookingDto> dtoPage = new PageImpl<>(dtoList, pageable, 1L);

        try (MockedStatic<RefundBookingSpecification> mockedStatic = mockStatic(RefundBookingSpecification.class)) {
            mockedStatic.when(() -> RefundBookingSpecification.searchRefund(refundSearchRequest))
                    .thenReturn(mock(Specification.class));
            when(refundRepository.findAll(any(Specification.class), any(Pageable.class))).thenReturn(entityPage);
            when(refundMapper.entityToDto(entityPage)).thenReturn(dtoPage);
            Page<RefundBookingDto> result = refundDao.searchRefundBookingRequest(refundSearchRequest, pageable);
            verify(refundRepository, times(1)).findAll(any(Specification.class), eq(pageable));
            assertNotNull(result);
        }
    }

expected: not <null> 

   public Page<RefundBookingDto> searchRefundBookingRequest(RefundSearchRequest refundSearchRequest, Pageable pageable) {
        logger.info("Searching refund bookings - Merchant ID: {}", refundSearchRequest.getMId());
        Specification<RefundBooking> specification = RefundBookingSpecification.searchRefund(refundSearchRequest);
        Page<RefundBooking> refundBookingPage = refundRepository.findAll(specification, pageable);
        List<String> atrnNumbers = refundBookingPage.getContent().stream()
                .map(RefundBooking::getAtrnNum)
                .collect(Collectors.toList());
        List<MerchantOrderPayment> merchantOrderPaymentList = merchantOrderPaymentDao.findByAtrnNumberIn(atrnNumbers);
        return refundMapper.mapPageWithPaymentsToDto(refundBookingPage, merchantOrderPaymentList);
    }

    default Page<RefundBookingDto> mapPageWithPaymentsToDto(Page<RefundBooking> refundBookingPage, List<MerchantOrderPayment> merchantOrderPaymentList) {

        Map<String, PayMode> payModeMap = merchantOrderPaymentList.stream()
                .collect(Collectors.toMap(MerchantOrderPayment::getAtrnNumber, MerchantOrderPayment::getPayMode));
        return refundBookingPage.map(refundBooking -> {
            RefundBookingDto refundBookingDto = entityToDto(refundBooking);

            PayMode mode = payModeMap.get(refundBooking.getAtrnNum());
            refundBookingDto.setPayMode(mode);

            return refundBookingDto;
        });
    }

@Test
void searchRefundBookingRequest() {

    List<RefundBooking> entityList = new ArrayList<>();
    RefundBooking booking = new RefundBooking();
    booking.setAtrnNum("ATRN123");
    entityList.add(booking);

    Page<RefundBooking> entityPage = new PageImpl<>(entityList, pageable, 1L);

    List<RefundBookingDto> dtoList = Collections.singletonList(new RefundBookingDto());
    Page<RefundBookingDto> dtoPage = new PageImpl<>(dtoList, pageable, 1L);

    List<MerchantOrderPayment> paymentList = new ArrayList<>();
    MerchantOrderPayment mop = new MerchantOrderPayment();
    mop.setAtrnNumber("ATRN123");
    mop.setPayMode(PayMode.UPI);
    paymentList.add(mop);

    try (MockedStatic<RefundBookingSpecification> mockedStatic = mockStatic(RefundBookingSpecification.class)) {

        mockedStatic.when(() -> RefundBookingSpecification.searchRefund(refundSearchRequest))
                .thenReturn(mock(Specification.class));

        when(refundRepository.findAll(any(Specification.class), eq(pageable)))
                .thenReturn(entityPage);

        when(merchantOrderPaymentDao.findByAtrnNumberIn(anyList()))
                .thenReturn(paymentList);

        when(refundMapper.mapPageWithPaymentsToDto(entityPage, paymentList))
                .thenReturn(dtoPage);

        Page<RefundBookingDto> result = refundDao.searchRefundBookingRequest(refundSearchRequest, pageable);

        assertNotNull(result);
        assertEquals(1, result.getContent().size());
    }
}
