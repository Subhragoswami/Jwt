   public static void createExcelWorkBook(HttpServletResponse response, String fileName, List<SheetData<?>> sheetDataList) throws IOException {
        Workbook workbook = new XSSFWorkbook();
        try {
            for (SheetData<?> sheetData : sheetDataList) {
                createSheet(workbook, sheetData.getSheetName(), sheetData.getHeaders(), sheetData.getData(), sheetData.getDataMapper());
            }
            writeWorkbookToResponse(response, workbook, fileName);
        } catch (IOException e) {
            throw new RuntimeException(e);
        } finally {
            closeWorkbook(workbook);
        }
    }


