 List<MerchantBankAccount> findAllByMId(String mId, Pageable pageable);

package com.epay.merchant.entity;

import jakarta.persistence.*;
import lombok.Builder;
import lombok.Data;

import java.util.UUID;

@Builder
@Data
@Entity
@Table(name = "MERCHANT_BANK_ACCOUNT")
public class MerchantBankAccount extends AuditEntity{
    @Id
    @GeneratedValue(strategy = GenerationType.UUID)
    @Column(name = "ID", nullable = false, updatable = false, unique = true)
    private UUID id;
    @Column(name="MID")
    private String mId;
    @Column(name="ACCOUNT_NAME")
    private Long accountName;
    @Column(name="BANK_NAME")
    private String bankName;
    @Column(name="IFSC_CODE")
    private String ifscCode;
    @Column(name="BRANCH_NAME")
    private String branchName;
    @Column(name="ACCOUNT_TYPE")
    private String accountType;
    @Column(name="ACCOUNT_NUMBER")
    private String accountNumber;
    @Column(name="IS_APPROVED")
    private int isApproved;
    @Column(name="IS_PRIMARY")
    private int isPrimary;
    @Column(name="APPROVED_REJECTED_BY")
    private String approvedRejectedBy;
    @Column(name="REJECTED_REASON")
    private String rejectedReason;
    @Column(name="APPROVED_REJECTED_ON")
    private String approvedRejectedOn;
    @Column(name="MERCHANT_BANK_ACCOUNT")
    private String merchantBankAccount;
    @Column(name="STATUS")
    private String status;

}
