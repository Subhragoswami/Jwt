    STATUS,
    CREATED_AT,
    CREATED_BY,
    UPDATED_AT,
    UPDATED_BY
) VALUES (
    SYS_GUID(), -- Generates a unique identifier
    SYS_GUID(), -- Replace with an actual REPORT_ID if necessary
    'Merchant_001', -- Example MID
    'DAILY', -- Frequency (e.g., DAILY, MONTHLY, YEARLY)
    'PDF', -- Report format (e.g., PDF, CSV)
    '9:00 AM', -- Schedule execution time
    1672531200000, -- NEXT_SCHEDULE_EXECUTION_TIME (Epoch timestamp in milliseconds)
    NULL, -- LAST_SCHEDULE_EXECUTION_TIME (nullable)
    'TO_BE_START', -- Status
    1672444800000, -- CREATED_AT (Epoch timestamp in milliseconds)
    'Admin', -- CREATED_BY
    NULL, -- UPDATED_AT (nullable)
