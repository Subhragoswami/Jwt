 @Query("SELECT a FROM AlertManagement a WHERE a.isViewed = false AND a.mId = :mId order by a.created_at desc")
    List<AlertManagement> findLatestAlerts(Pageable pageable, @Param("mId") String mId);

 public List<AlertManagementResponse> getUnreadAlerts(String mid){
        return alertManagementMapper.alertManagementEntityToResponse(alertManagementRepository.findLatestAlerts(PageRequest.of(0, 50), mid));
    }

package com.epay.merchant.entity;

import jakarta.persistence.*;
import lombok.*;

import java.util.UUID;

/**
