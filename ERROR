CREATE TABLE USER_MENU_PERMISSION (
    ID	RAW(16) DEFAULT SYS_GUID() PRIMARY KEY NOT NULL,
    PERMISSION_ID	RAW(16) NOT NULL,
    USER_ID	RAW(16) NOT NULL,
    MENU_ID	RAW(16) NOT NULL,
    CREATED_DATE	NUMBER NOT NULL,
    UPDATE_DATE	NUMBER NOT NULL,
    CONSTRAINT UK_USER_MENU_PERMISSION UNIQUE (PERMISSION_ID, USER_ID, MENU_ID), -- Unique KEY
    CONSTRAINT FK_USER_MENU_PERMISSION  FOREIGN KEY (PERMISSION_ID) REFERENCES USER_PERMISSION(ID), -- Foreign Key to `USER_PERMISSION` table
    CONSTRAINT FK_USER_MENU_PERMISSION_USER  FOREIGN KEY (USER_ID) REFERENCES MERCHANT_USER(ID), -- Foreign Key to `MERCHANT_USER` table
    CONSTRAINT FK_USER_MENU_PERMISSION_MENU  FOREIGN KEY (MENU_ID) REFERENCES MENU_INFO(ID) -- Foreign Key to `MENU_INFO` table
);

