 if (reportManagementRequest.getDurationFromDate() < subtractMonth(DURATION_FROM_DATE_LIMIT)) {
            errorDtoList.add(ErrorDto.builder().errorCode(INVALID_ERROR_CODE).errorMessage(MessageFormat.format(INVALID_ERROR_MESSAGE, DURATION_FROM_DATE, MessageFormat.format("durationFromDate can't be less than past {} months", DURATION_FROM_DATE_LIMIT))).build());
        }
