@Entity
@Getter
@Setter
@Builder
@AllArgsConstructor
@NoArgsConstructor
@EqualsAndHashCode(callSuper = true)
@Table(name = "CAPTCHA_MANAGEMENT")
public class Captcha extends AuditEntityByDate {

    @Id
    @GeneratedValue(strategy = GenerationType.UUID)
    private UUID id;
